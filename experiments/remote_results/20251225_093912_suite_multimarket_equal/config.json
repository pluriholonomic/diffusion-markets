{
  "benchmark": "multimarket",
  "data": {
    "n_test": 20000,
    "n_train": 80000
  },
  "eval": {
    "curve_every": 500,
    "no_box": false,
    "pred_noise": 0.15
  },
  "infer": {
    "agg": "mean",
    "eta": 0.0,
    "mc": 16,
    "sample_steps": [
      4,
      8,
      16,
      32
    ]
  },
  "spec": {
    "d": 32,
    "noise": 0.25,
    "seed": 1,
    "structure": "equal"
  },
  "train": {
    "T": 64,
    "batch_size": 2048,
    "beta_end": 0.02,
    "beta_start": 0.0001,
    "depth": 2,
    "device": "cuda",
    "dropout": 0.0,
    "heads": 4,
    "hidden_dim": 256,
    "label_eps": 1e-06,
    "lr": 0.0002,
    "time_dim": 64,
    "train_steps": 2000
  }
